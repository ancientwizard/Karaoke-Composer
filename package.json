{
  "name": "karaoke-composer",
  "version": "0.0.2",
  "description": "A Vue 3 Karaoke Application",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "cypress open",
    "test:e2e:headless": "cypress run",
    "debug": "tsx",
    "debug:218ms": "tsx src/debug/debug218ms.ts",
    "debug:219ms": "tsx src/debug/debug219ms.ts",
    "debug:1127ms": "tsx src/debug/debugBoundary1127.ts",
    "debug:test-timing": "tsx src/debug/debugTestTiming.ts",
    "debug:multiple-words": "tsx src/debug/debugMultipleWords.ts",
    "debug:syllable-detection": "tsx src/debug/debugSyllableDetection.ts",
    "debug:syllables": "tsx src/debug/debugSyllables.ts",
    "debug:test-exact": "tsx src/debug/debugTestExact.ts",
    "debug:final-verification": "tsx src/debug/finalVerification.ts",
    "debug:integration": "tsx src/debug/integrationTest.ts",
    "debug:analyze-overlaps": "tsx src/debug/analyzeTimingOverlaps.ts",
    "deploy-gh-pages": "gh-pages -d dist -m \"Deploy v0.0.1\" --dest v0.0.1 --no-push -verbose",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "type-check": "vue-tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "size": "find src -name '*.ts' -o -name '*.vue' -o -name '*.js' | xargs wc -l | tail -1"
  },
  "dependencies": {
    "bootstrap": "^5.3.0",
    "bootstrap-icons": "^1.13.1",
    "vue": "^3.3.0",
    "vue-router": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "@jest/globals": "^29.5.0",
    "@rushstack/eslint-patch": "^1.3.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.3.0",
    "@vitejs/plugin-legacy": "^4.1.0",
    "@vitejs/plugin-vue": "^4.2.0",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.4.0",
    "@vue/tsconfig": "^0.4.0",
    "@vue/vue3-jest": "^29.2.0",
    "babel-jest": "^29.5.0",
    "cypress": "^12.17.0",
    "eslint": "^8.42.0",
    "eslint-plugin-vue": "^9.15.0",
    "gh-pages": "^6.3.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-transform-stub": "^2.0.0",
    "npm-run-all": "^4.1.0",
    "ora": "^9.0.0",
    "prettier": "^3.6.2",
    "terser": "^5.18.0",
    "ts-jest": "^29.1.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vite": "^4.3.0",
    "vue-tsc": "^3.1.1"
  },
  "keywords": [
    "vue3",
    "karaoke",
    "typescript",
    "vite",
    "bootstrap"
  ],
  "author": "Ancient Wizard",
  "license": "ISC"
}